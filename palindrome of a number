#include<stdio.h>
int main()
{
    int remainder,reverse=0,original,n;
    printf("enter the number");
    scanf("%d",&n);
    original=n;
    while(n>0)
    {
    remainder=n%10;
    reverse=(reverse*10)+remainder;
    n=n/10;
}
if(original==reverse)
{
    printf(" it is palindrom");
}
else
{
    printf("it is not a palindrom");
}
return 0;
}
