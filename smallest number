#include<stdio.h>

int main()
{
    int a, b, c;
    printf("enter the values of three numbers:");
    scanf("%d %d %d", &a, &b, &c);
    
    if(a<b && b<c)
    {
        printf("%d is the smallest number",a);
    }
    else if("b<c")
    {
        printf("%d is the smallest number",b);
    }
    else
    {
        printf("%d is the smallest number",c);
    }
    
    return 0;
}
